{
  "name": "sonic-red-dragon-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "prestart": "node scripts/fix-all-docs-issues.js",
    "start": "docusaurus start",
    "fix-all-docs": "node ./scripts/fix-all-docs-issues.js",
    "fix-all-mermaid": "node ./scripts/fix-all-mermaid-issues-v2.js",
    "verify-mermaid": "node ./scripts/verify-mermaid-syntax.js",
    "check-health": "node ./scripts/check-site-health.js",
    "fix-missing-images": "node ./scripts/fix-missing-images.js",
    "fix-links": "node ./scripts/fix-remaining-links.js",
    "fix-markdown-links": "node ./scripts/fix-markdown-links.js",
    "fix-all-markdown-links": "node ./scripts/fix-all-markdown-links.js",
    "fix-broken-links": "node ./scripts/fix-broken-links.js",
    "fix-mermaid": "node ./scripts/fix-mermaid-rendering.js",
    "fix-mermaid-syntax": "node ./scripts/fix-mermaid-syntax.js",
    "fix-build-memory-issues": "node ./scripts/fix-build-memory-issues.js",
    "fix-mermaid-memory-issues": "node ./scripts/fix-mermaid-memory-issues.js",
    "fix-and-build-docs": "node ./scripts/fix-and-build-docs.js",
    "fix-and-build-extreme": "node ./scripts/fix-and-build-extreme.js",
    "enhance-mermaid-interactive": "node ./scripts/enhance-mermaid-interactive.js",
    "prebuild": "npm run fix-all-mermaid && npm run fix-all-docs",
    "build": "npm run fix-missing-images && npm run fix-links && npm run fix-all-markdown-links && npm run fix-mermaid-diagrams && npm run enhance-mermaid-interactive && docusaurus build",
    "optimized-build": "node ./scripts/optimize-build.js",
    "debug-build": "node ./scripts/debug-build.js",
    "rebuild-with-mermaid-fix": "node ./scripts/rebuild-with-mermaid-fix.js",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "serve-build": "node ./scripts/serve-docs.js",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "pre-deploy-check": "node ./scripts/pre-deploy-check.js",
    "deploy-ci": "npm run pre-deploy-check && npm run fix-missing-images && npm run fix-links && npm run fix-all-markdown-links && npm run fix-broken-links && npm run fix-mermaid && npm run optimized-build && npm run deploy",
    "fix-mermaid-diagrams": "node ./scripts/fix-mermaid-diagrams.js",
    "build-with-increased-memory": "node ./build-with-increased-memory.js",
    "build-with-extreme-optimization": "node ./build-with-extreme-optimization.js",
    "fix-docs": "node scripts/fix-all-docs-issues.js",
    "verify-docs": "node scripts/verify-fixes.js"
  },
  "dependencies": {
    "@docusaurus/core": "^3.8.0",
    "@docusaurus/plugin-client-redirects": "^3.8.0",
    "@docusaurus/preset-classic": "^3.8.0",
    "@docusaurus/theme-mermaid": "^3.8.0",
    "@mdx-js/react": "3.0.0",
    "@react-three/drei": "^9.88.11",
    "@react-three/fiber": "^8.15.11",
    "@react-three/postprocessing": "^2.15.11",
    "@types/three": "^0.158.3",
    "animejs": "^3.2.2",
    "clsx": "2.0.0",
    "dayjs": "^1.11.10",
    "docusaurus-plugin-react-docgen-typescript": "^1.3.0",
    "framer-motion": "^10.16.5",
    "glob": "^10.4.5",
    "gsap": "^3.12.3",
    "lottie-react": "^2.4.0",
    "mermaid": "^11.6.0",
    "prism-react-renderer": "2.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "^9.5.3",
    "react-parallax": "^3.5.1",
    "react-spring": "^9.7.3",
    "styled-jsx": "^5.1.2",
    "three": "^0.158.0",
    "unist-util-visit": "^4.1.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.8.0",
    "@docusaurus/tsconfig": "^3.8.0",
    "@docusaurus/types": "3.7.0",
    "typescript": "5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}